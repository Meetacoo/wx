<template name="starTemplate">
  <view class="start-box">
    <block wx:for="{{stars}}" wx:for-item="num" wx:key="">
      <image wx:if="{{num == 1}}" src='/images/icon/star2.jpg' class='star-img'></image>
      <image wx:else src='/images/icon/star1.jpg' class='star-img'></image>
    </block>
    <text class='score'> {{score}}</text>
  </view>
</template>